  <changelog>
    <change>
      <date>2005</date>
      <author>Arthur Frenslich (thuromail@thuro.at)</author>
      <en>Created .air file, 3D model and texture.</en>
    </change>
    <change>
      <date>Unknown</date>
      <author>CRRCSim 0.9.6beta3</author>
